{
  "name": "@damsenviet/kle",
  "version": "1.0.1",
  "description": "A JavaScript library for interacting with KLE JSON and KLE data structures.",
  "files": ["dist/*"],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "npm run build:umd && npm run build:cjs && npm run build:esm",
    "clean": "npm run clean:umd && npm run clean:cjs && npm run clean:esm && rimraf dist",
    "build:umd": "node scripts/build.js",
    "watch:umd": "node scripts/watch.js",
    "clean:umd": "rimraf dist/umd",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "watch:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean:cjs": "rimraf dist/cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm",
    "watch:esm": "tsc -w --module es6 --outDir dist/esm",
    "clean:esm": "rimraf dist/esm",
    "test:esm": "jest --passWithNoTests",
    "dev:docs": "typedoc --readme none --out static/api/ package/index.ts && gatsby develop",
    "build:docs": "typedoc --readme none --out static/api/ package/index.ts && gatsby build",
    "clean:docs": "rimraf static/api/ && gatsby clean",
    "test:docs": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamSenViet/kle-js.git"
  },
  "author": "DamSenViet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DamSenViet/kle-js/issues"
  },
  "homepage": "https://github.com/DamSenViet/kle-js#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@rocketseat/gatsby-theme-docs": "^2.4.0",
    "@types/jest": "^26.0.23",
    "babel-loader": "^8.2.3",
    "clear": "^0.1.0",
    "gatsby": "^3.0.4",
    "gatsby-plugin-canonical-urls": "^3.0.0",
    "gatsby-plugin-google-analytics": "^3.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^3.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-remark-typedoc-symbol-links": "^2.0.0",
    "gatsby-source-typedoc": "^1.4.0",
    "jest": "^27.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "signale": "^1.4.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.62.1",
    "webpack-format-messages": "^3.0.1"
  },
  "dependencies": {
    "ajv": "^8.6.2"
  },
  "unpkg": "dist/umd/index.js"
}
